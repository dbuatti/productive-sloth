{
  "compilerOptions": {
    "lib": ["deno.ns", "deno.window", "deno.unstable"],
    "strict": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "target": "es2020",
    "jsx": "react-jsx",
    "allowJs": true,
    "checkJs": true,
    "noEmit": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "types": ["deno_graph"]
  },
  "include": ["./**/*.ts"]
}